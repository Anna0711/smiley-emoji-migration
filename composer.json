{
    "name": "youthweb/smiley-emoji-migration",
    "type": "library",
    "description": "Migration from smilies to emojis on Youthweb",
    "homepage": "https://github.com/youthweb/smiley-emoji-migration",
    "keywords": ["youthweb", "smiley", "smilies", "emoji", "emojis", "migration"],
    "license": "GPL3",
    "require": {
        "php": "^5.6 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.6"
    },
    "config": {
        "vendor-dir": "php/vendor"
    },
    "autoload": {
        "psr-4": {
            "Youthweb\\SmileyEmojiMigration\\": "php/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Youthweb\\SmileyEmojiMigration\\Tests\\": "php/tests"
        }
    }
}
